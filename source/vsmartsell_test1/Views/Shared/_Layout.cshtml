<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title Vsmartsell Dashboard</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/se7en-font.css" rel="stylesheet" />
    <link href="~/Scripts/bootstrap-editable.css" rel="stylesheet" />
    <link href="~/Scripts/style.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
</head>
<body class="page-header-fixed bg-1">
    <div class="navbar navbar-fixed-top scroll-hide" style="overflow: visible;">
        <div class="container-fluid top-bar">
            <div class="pull-right">
               
            </div>
            <button class="navbar-toggle">
                <span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span>
            </button>
            <a href="/vsmartsell/index" class="resetpageandtype" style="font-size:30px;color:red;font-weight:bold;font-family:'Arial Unicode MS'"><img src="~/images/logo.png" style="width:40px" />Vsmartsell Dashboard</a>
            @*<form class="navbar-form form-inline col-lg-2 hidden-xs" style="float:right">
                <input class="form-control" placeholder="Search" type="text">
            </form>*@
        </div>
        <div class="container-fluid main-nav clearfix">
            <div class="nav-collapse">
                <ul class="nav">
                    <li>
                        <a id="trangchu" class="resetpageandtype"  onclick="resetpage()" href="/vsmartsell/index"><span aria-hidden="true" class="se7en-home"></span>Trang chủ</a>
                    </li>
                    <li>
                        <a id="bangdieukhien" href="/vsmartsell/controlpanel"><span aria-hidden="true" class="se7en-tables"></span>Bảng điều khiển</a>
                    </li>
               
                </ul>
            </div>
        </div>
    </div>
    <div class="container-fluid main-nav clearfix">
        @RenderBody()
        <hr />
        <footer>
           
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    <script src="~/Scripts/angular.min.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    <script src="~/Scripts/jquery.session.js"></script>
    <script type="text/javascript">
        
    </script>
}